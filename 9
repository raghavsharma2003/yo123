function removeDuplicatesUsingSet(arr) {
  return [...new Set(arr)];
}

const names = ["John", "Jane", "Peter", "John", "Alice"];
const uniqueNames = removeDuplicatesUsingSet(names);
console.log(uniqueNames); // Output: ["John", "Jane", "Peter", "Alice"]




